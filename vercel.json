{
  "version": 2,
  "builds": [
    {
      "src": "link-tracker-dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "npm install --legacy-peer-deps"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/link-tracker-dashboard/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/link-tracker-dashboard/assets/$1"
    },
    {
      "src": "/7th_brain_logo.png",
      "dest": "/link-tracker-dashboard/7th_brain_logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/link-tracker-dashboard/index.html"
    }
  ]
}

